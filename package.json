  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "startboth": "node pm2 && ng build --watch",
    "build": "node --max_old_space_size=8590 node_modules/@angular/cli/bin/ng build  --output-hashing=all",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc": "compodoc src -p tsconfig.json -s",
    "localnode": "nodemon src/server/app.js --watch",
    "dev": "ng serve --configuration=dev --port 3000 --ssl",
    "pm": "ng serve --configuration=pm --port 3000 --ssl",
    "ppt": "ng serve --configuration=ppt --port 3000 --ssl",
    "cfit": "ng serve --configuration=cfit --port 3000 --ssl",
    "prod": "ng serve --configuration=production --port 3000 --ssl",
    "train": "ng serve --configuration=train --port 3000 --ssl",
    "cctdevpm": "concurrently  \"npm run localnode\" \"npm run dev\"",
    "cctdevcfit": "concurrently  \"npm run localnode\" \"npm run cfit\"",
    "cctdevprod": "concurrently  \"npm run localnode\" \"npm run prod\"",
    "cctdevppt": "concurrently  \"npm run localnode\" \"npm run ppt\"",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start"
  },
